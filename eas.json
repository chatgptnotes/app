{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "bundleIdentifier": "com.drmurali.aihairstylegenerator",
        "appStoreConnect": {
          "ascAppId": "your-asc-app-id",
          "ascApiKey": "path-to-your-asc-api-key.json"
        }
      },
      "android": {
        "package": "com.drmurali.aihairstylegenerator",
        "serviceAccountKeyPath": "path-to-your-service-account-key.json"
      }
    }
  }
}